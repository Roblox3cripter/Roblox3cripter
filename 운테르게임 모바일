local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

local UI = Material.Load({
     Title = "Untel23's Game",
     Style = 3,
     SizeX = 400,
     SizeY = 300,
     Theme = "Dark"
})

local Page = UI.New({
    Title = "Main"
})

local gunp = nil
local carp = nil
local drinkp = nil

for i, v in pairs(game.workspace:GetDescendants()) do
    if v:IsA("Part") then
        if v.CFrame == CFrame.new(-75, 12.5, 275, -1, 0, 0, 0, 0, 1, 0, 1, -0) then
            gunp = v
        end
    end
end

for i, v in pairs(game.workspace:GetDescendants()) do
    if v:IsA("Part") then
        if v.CFrame == CFrame.new(-111.5, 11.5, 278.5, 0, -1, -0, -1, 0, -0, 0, 0, -1) then
            carp = v
        end
    end
end

for i, v in pairs(game.workspace:GetDescendants()) do
    if v:IsA("Part") then
        if v.CFrame == CFrame.new(-247.5, 15.3999119, 164, 0, 0, -1, 0, 1, 0, 1, 0, 0) then
            drinkp = v
        end
    end
end

Page.Button({
    Text = "Auto Gun Giver",
    Callback = function()
while true do
gunp.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
gunp.Transparency = 1
wait(0.05)
end
    end
})

Page.Button({
    Text = "Steal Gun",
    Callback = function()
for i, v in pairs(game.Workspace:GetDescendants()) do
if v:IsA("Tool") then
if v.Name == "pistol" then
v.Parent = game.Players.LocalPlayer.Backpack
end
end
end
    end
})

Page.Button({
    Text = "Auto Car Spawn",
    Callback = function()
while true do
carp.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
carp.Transparency = 1
wait(0.05)
end
    end
})

Page.Button({
    Text = "Click All Button",
    Callback = function()
for i, v in pairs(Workspace:GetDescendants()) do
	if v:IsA("ClickDetector") then
		fireclickdetector(v)
	end
end
    end
})

Page.Button({
    Text = "Crazy Car",
    Callback = function()
       for i, v in pairs(Workspace:GetDescendants()) do
if v:IsA("Model") then
for i, v in pairs(v:GetDescendants()) do
if v:IsA("Model") then
for i, v in pairs(v:GetChildren()) do
if v.Name == 'Configurations' then
v.Speed.Value = 100
v.Height.Value = 25
v.Suspension.Value = 20
v.TurnSpeed.Value = 5
end
end
end
end
end
end

for i, v in pairs(Workspace:GetDescendants()) do
if v:IsA("Model") then
for i, v in pairs(v:GetDescendants()) do
if v:IsA("Model") then
for i, v in pairs(v:GetChildren()) do
if v.Name == 'DriveSeat' then
v.MaxSpeed = 100
end
end
end
end
end
end
    end
})

Page.Button({
    Text = "Auto Click Drink Button",
    Callback = function()
while true do
for i, v in pairs(drinkp.Parent:GetDescendants()) do
if v:IsA("ClickDetector") then
fireclickdetector(v)
end
end
wait(0.1)
end
    end
})

Page.TextField({
    Text = "Kill Player (Equiped Gun Needed)",
    Callback = function(value)
local args = {
    [1] = game:GetService("Players")[value].Character.Humanoid
}

game:GetService("Players").LocalPlayer.Character.pistol.RemoteEvent:FireServer(unpack(args))
    end
})
